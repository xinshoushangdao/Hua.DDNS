{
  "ConnectionStrings": {
    "pgConnection": "Host=127.0.0.1;Port=5432;Database=Worker;Username=Worker;Password=123456;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "App": {
    "AppJob": {
      "Corn": "* * * * * ?" //https://cron.qqe2.com/
    },

    "Domain": {
      "Platform": "Tencent",
      // 阿里云的 Access Id
      "Id": "Id",
      // 阿里云的 Access Key
      "Key": "Key",
      // 主域名
      "domain": "demoDomain.cn",
      // 子域名前缀
      "subDomainArray": [ "bjb", "git" ],
      // 记录类型
      "type": "A",
      //间隔时间 秒
      "time": "30"
    }
  }
}